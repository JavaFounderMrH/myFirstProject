package com.shb.web.dto.room;

import java.io.Serializable;
import java.util.List;

import com.shb.web.mobile.room.RoomEvaluatePicture;
import com.shb.web.mobile.user.User;

@SuppressWarnings("serial")
public class RoomEvaluateDto implements Serializable{
	
	private User user; // 用户评论信息
	
	private List<RoomEvaluatePicture> roomEvaluatePictureList; // 评论图片
	
	private Integer bingPage;
	
	private Integer pages;

	public Integer getPages() {
		return pages;
	}

	public void setPages(Integer pages) {
		this.pages = pages;
	}

	public Integer getBingPage() {
		return bingPage;
	}

	public void setBingPage(Integer bingPage) {
		this.bingPage = bingPage;
	}

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}

	public List<RoomEvaluatePicture> getRoomEvaluatePictureList() {
		return roomEvaluatePictureList;
	}

	public void setRoomEvaluatePictureList(List<RoomEvaluatePicture> roomEvaluatePictureList) {
		this.roomEvaluatePictureList = roomEvaluatePictureList;
	}

}
