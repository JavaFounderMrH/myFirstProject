/*  ==      home    ==  */
#homeBanner{
    width: 100%;
    margin-top: 0.435rem;
}
#homeBanner .swiper-slide{
    width: 100%;
    height: 2rem;
}
.banner1{
    background: url("../img/home/banner1.png") no-repeat;
    background-size: cover;
}
.banner2{
    background: url("../img/home/banner2.png") no-repeat;
    background-size: cover;
}
.banner3{
    background: url("../img/home/banner3.png") no-repeat;
    background-size: cover;
}
.banner4{
    background: url("../img/home/banner4.png") no-repeat;
    background-size: cover;
}
#homeBanner .swiper-pagination-bullet {
    width: 0.05rem;
    height: 0.05rem;
    opacity: 1;
    margin: 0 0.045rem;
    background: rgba(255,255,255,0.8);
}
#homeBanner .swiper-pagination-bullet-active {
    width: 0.18rem;
    opacity: 1;
    background: rgba(255,255,255,1);
    border-radius: 20px;
}
.fangyuan{
    width: 94.66%;
    margin: 0 0.1rem;
}
.fangyuan h2, .fangxing h2{
