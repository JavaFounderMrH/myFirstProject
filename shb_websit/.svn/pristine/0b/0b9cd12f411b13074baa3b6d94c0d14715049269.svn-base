.pc-bannerWrap .swiper-container{
    width: 100%;
    /*padding-top: 4%;*/
}
.pc-bannerWrap .swiper-slide{
    -webkit-transition: transform 1.0s;
    -moz-transition: transform 1.0s;
    -ms-transition: transform 1.0s;
    -o-transition: transform 1.0s;
    -webkit-transform: scale(0.7);
    transform: scale(0.7);
}
.pc-bannerWrap .swiper-slide-active, .pc-bannerWrap .swiper-slide-duplicate-active{
    -webkit-transform: scale(1);
    transform: scale(1);
}
.pc-bannerWrap .none-effect{
    -webkit-transition: none;
    -moz-transition: none;
    -ms-transition: none;
    -o-transition: none;
}
.pc-bannerWrap .swiper-slide a{
    background: #fff;
    padding:10px;
    display: block;
    border-radius: 14px;
    cursor: context-menu;
}
.pc-bannerWrap .swiper-slide-active a:after{
    background: none;
}
.pc-bannerWrap .swiper-slide img{
    width: 100%;
    border-radius: 14px;
    display: block;
}
.pc-bannerWrap .swiper-pagination{
    position: absolute;
    left: 25%;
    bottom: 4%;
}
.pc-bannerWrap .swiper-pagination-bullet{
    background: #bbb!important;
    margin: 0 9px!important;
    width: 50px!important;
    height: 4px!important;
    opacity: 1;
    margin-bottom: 4px;
    border-radius: 0!important;
}
.pc-bannerWrap .swiper-pagination-bullet-active{
    width: 50px!important;
    height: 4px!important;
    background: #40d8d7!important;
    margin-bottom: 0;
}
