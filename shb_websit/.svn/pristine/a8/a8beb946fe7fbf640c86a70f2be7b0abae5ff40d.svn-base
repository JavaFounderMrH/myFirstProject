package com.shb.web.dto.room;

import java.io.Serializable;

public class RoomPriceCalendarDto implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	private Double price; // 房间每天的价格

	private Long sriRid; // 房间ID

	private Integer roomCheck; // 是否有房

	private String time; // 日期

	private Integer nums; // 房间套数

	private Integer type;

	public Double getPrice() {
		return price;
	}

	public void setPrice(Double price) {
		this.price = price;
	}

	public Integer getType() {
		return type;
	}

	public void setType(Integer type) {
		this.type = type;
	}

	public Long getSriRid() {
		return sriRid;
	}

	public void setSriRid(Long sriRid) {
		this.sriRid = sriRid;
	}

	public Integer getRoomCheck() {
		return roomCheck;
	}

	public void setRoomCheck(Integer roomCheck) {
		this.roomCheck = roomCheck;
	}

	public String getTime() {
		return time;
	}

	public void setTime(String time) {
		this.time = time;
	}

	public Integer getNums() {
		return nums;
	}

	public void setNums(Integer nums) {
		this.nums = nums;
	}

}
